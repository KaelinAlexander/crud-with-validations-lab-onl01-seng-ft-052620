<%= form_for @song do |f| %>
<%= f.label 'Title:' %><br>
<%= f.text_field :title %>
<%= f.label 'Released:' %><br>
<%= f.text_field :released %>
<%= f.label 'Release Year:' %><br>
<%= f.text_field :release_year %>
<%= f.label 'Artist:' %><br>
<%= f.text_field :artist_name %>
<%= f.label 'Genre:' %><br>
<%= f.text_field :genre %>
<%= f.submit %>
<% end %>
